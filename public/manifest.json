{
  "manifest_version": 3,
  "name": "BootDevBattle Starter",
  "version": "0.1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.boot.dev/learn/*"],
      "js": ["content.js"]
    }
  ]
}
